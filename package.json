{
  "name": "chopfast",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=1024 vite build",
    "build:production": "cross-env NODE_OPTIONS=--max-old-space-size=1024 NODE_ENV=production vite build",
    "build:windows": "set NODE_OPTIONS=--max-old-space-size=1024 && vite build",
    "build:unix": "NODE_OPTIONS='--max-old-space-size=1024' vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 4173 --host 0.0.0.0",
    "start": "vite preview --port 4173 --host 0.0.0.0",
    "start:prod": "cross-env PORT=4173 vite preview --host 0.0.0.0",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "translate": "node scripts/translate.js",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "rimraf node_modules package-lock.json dist",
    "reinstall": "npm run clean:all && npm install",
    "postinstall": "npm run compile"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@hookform/resolvers": "^5.0.1",
    "@lingui/core": "^5.3.2",
    "@lingui/macro": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@react-icons/all-files": "^4.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.6",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.10.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "flowbite": "^3.1.2",
    "framer-motion": "^12.12.1",
    "gsap": "^3.13.0",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "icons": "^1.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.509.0",
    "motion": "^12.12.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-i18next": "^15.5.2",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@eslint/js": "^9.25.0",
    "@lingui/babel-plugin-lingui-macro": "^5.3.2",
    "@lingui/cli": "^5.3.2",
    "@lingui/vite-plugin": "^5.3.2",
    "@rollup/rollup-win32-x64-msvc": "^4.44.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.1",
    "tailwindcss": "^4.1.6",
    "vite": "^5.4.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
