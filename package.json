    "@lingui/react": "^4.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "date-fns": "^3.0.6",
    "firebase": "^10.7.1",
    "flowbite": "^2.2.1",
    "framer-motion": "^10.18.0",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
